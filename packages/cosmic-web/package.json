{
  "name": "@cosui/cosmic-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rimraf dist",
    "dev": "vite",
    "prebuild": "cd ../../ && pnpm build && pnpm build",
    "gen": "tsx ./scripts/screenshot.ts",
    "genall": "tsx ./scripts/screenshot.ts all",
    "build": "NODE_ENV_TYPE=production vite build",
    "dev:ssr": "RENDER_TYPE=ssr vite",
    "dev:cjs": "cd ../../ && NODE_ENV=production node ./scripts/build-cjs.mjs -w",
    "build:cjs": "cd ../../ && NODE_ENV=production node --max-old-space-size=8192 ./scripts/build-cjs.mjs",
    "build:cjs:cosmic": "cd ../../ && NODE_ENV=production node ./scripts/build-cjs.mjs -p cosmic",
    "build:cjs:dqa": "cd ../../ && NODE_ENV=production node ./scripts/build-cjs.mjs -p cosmic-dqa",
    "build:cjs:card": "cd ../../ && NODE_ENV=production node ./scripts/build-cjs.mjs -p cosmic-card",
    "build:cjs:shop": "cd ../../ && NODE_ENV=production node ./scripts/build-cjs.mjs -p cosmic-shop",
    "png2webp": "node ./scripts/png2webp.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-virtual": "^3.0.2",
    "cross-env": "^7.0.3",
    "decache": "^4.6.2",
    "less": "~3.13.1",
    "rollup": "^4.30.0",
    "rollup-plugin-amd": "~4.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tmp": "^0.2.3",
    "tsx": "^4.19.1",
    "vite": "~4.4.9",
    "vite-plugin-md-san": "1.0.5",
    "vite-plugin-prismjs": "~0.0.8"
  },
  "dependencies": {
    "@cosui/cosmic": "workspace:*",
    "@cosui/cosmic-card": "workspace:~",
    "@cosui/cosmic-dqa": "workspace:~",
    "@cosui/cosmic-education": "workspace:~",
    "@cosui/cosmic-medical": "workspace:~",
    "@cosui/cosmic-shop": "workspace:~",
    "@cosui/cosmic-dynamic-ui": "workspace:~",
    "@cosui/icon": "^1.0.2",
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/lang-markdown": "^6.2.2",
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.21.3",
    "@uiw/codemirror-theme-github": "^4.21.19",
    "codemirror": "^6.0.1",
    "lottie-web": "^5.7.3",
    "marklang": "workspace:~",
    "nprogress": "~0.2.0",
    "plyr": "^3.7.8",
    "prismjs": "~1.29.0",
    "puppeteer": "^23.1.1",
    "puppeteer-core": "^23.1.1",
    "qrcanvas": "~3.1.2",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "san": "^3.14.1",
    "san-router": "~2.0.1",
    "san-ssr": "3.14.3",
    "sharp": "^0.33.5",
    "unified": "^11.0.3",
    "versions-compare": "~1.0.5"
